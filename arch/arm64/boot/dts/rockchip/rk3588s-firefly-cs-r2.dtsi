        default-brightness-level = <150>;
    };
};

/* usb2 host */
&u2phy2 {
	status = "disabled";
};

&u2phy2_host {
	status = "disabled";
};

&u2phy3 {
	status = "disabled";
};

&u2phy3_host {
	status = "disabled";
};

/* typec0 */
&usbdp_phy0 {
       status = "disabled";
};
&usbdp_phy0_dp {
       status = "disabled";
};
&usbdp_phy0_u3 {
       status = "disabled";
};

&u2phy0_otg {
	/delete-property/ rockchip,typec-vbus-det;
};

&usbdrd_dwc3_0 {
       dr_mode = "peripheral";
       phys = <&u2phy0_otg>;
       phy-names = "usb2-phy";
       maximum-speed = "high-speed";
};

/*40 pins*/
&pwm3 {
    pinctrl-0 = <&pwm3m1_pins>;
    status = "okay";
};


/*
 * mipi_dcphy1 needs to be enabled
 * when dsi1 is enabled
 */
&mipi_dcphy1 {
    status = "okay";
};

&dsi1_in_vp2 {
    status = "disabled";
};

&dsi1_in_vp3 {
    status = "okay";
};

&route_dsi1 {
    status = "okay";
    connect = <&vp3_out_dsi1>;
};
&dsi1 {
    status = "okay";
    //rockchip,lane-rate = <1000>;
    dsi1_panel: panel@0 {
        status = "okay";
        compatible = "simple-panel-dsi";
        reg = <0>;
        backlight = <&backlight>;
	enable-gpios = <&gpio0 RK_PD3 GPIO_ACTIVE_HIGH>;
        reset-gpios = <&gpio4 RK_PA7 GPIO_ACTIVE_LOW>;

        enable-delay-ms = <50>;
        prepare-delay-ms = <200>;
        reset-delay-ms = <50>;
        init-delay-ms = <55>;
        unprepare-delay-ms = <50>;
        disable-delay-ms = <20>;
        mipi-data-delay-ms = <200>;
        size,width = <120>;
        size,height = <170>;
        dsi,flags = <(MIPI_DSI_MODE_VIDEO | MIPI_DSI_MODE_VIDEO_BURST | MIPI_DSI_MODE_LPM | MIPI_DSI_MODE_EOT_PACKET)>;
        dsi,format = <MIPI_DSI_FMT_RGB888>;
        dsi,lanes  = <4>;

        panel-init-sequence = [
            //39 00 04 B9 83 10 2E
            // 15 00 02 CF FF
            05 78 01 11
            05 32 01 29
            //15 00 02 35 00
        ];

        panel-exit-sequence = [
            05 00 01 28
            05 00 01 10
        ];

        disp_timings0: display-timings {
            native-mode = <&dsi1_timing0>;
            dsi1_timing0: timing0 {
                clock-frequency = <72600000>;//<80000000>;
                hactive = <1080>;//<768>;
                vactive = <1920>;
                hsync-len = <14>;   //20, 50,10
                hback-porch = <26>; //50, 56,10
                hfront-porch = <32>;//50, 30,180
                vsync-len = <8>;//4
                vback-porch = <20>;//4
                vfront-porch = <80>;//8
                hsync-active = <0>;
                vsync-active = <0>;
                de-active = <0>;
                pixelclk-active = <0>;
            };
        };

        ports {
            #address-cells = <1>;
            #size-cells = <0>;

            port@0 {
                reg = <0>;
                panel_in_dsi1: endpoint {
                    remote-endpoint = <&dsi1_out_panel>;
                };
            };
        };
    };

    ports {
        #address-cells = <1>;
        #size-cells = <0>;

        port@1 {
            reg = <1>;
            dsi1_out_panel: endpoint {
            remote-endpoint = <&panel_in_dsi1>;
            };
        };
    };
};

/* hdmi0 */
&hdmi0 {
    enable-gpios = <&gpio4 RK_PB6 GPIO_ACTIVE_HIGH>;
    status = "okay";
};

&hdmi0_in_vp0 {
    status = "okay";
};

&hdmi0_sound {
        status = "okay";
};

&hdptxphy_hdmi0 {
    status = "okay";
};

&route_hdmi0{
    status = "okay";
};

&sdmmc {
    status = "disabled";
    vmmc-supply = <&vcc_3v3_sd_s0>;
};

&gmac1{
	phy-mode = "rgmii";
 	snps,reset-gpio = <&gpio3 RK_PB7 GPIO_ACTIVE_LOW>;
	tx_delay = <0x45>;
	rx_delay = <0x32>;
	status = "okay";
};

&i2c6 {
	status = "okay";
   	pinctrl-names = "default";
    pinctrl-0 = <&i2c6m3_xfer>;

    pc9202@3c {
        status = "okay";
        compatible = "firefly,pc9202";
        reg = <0x3c>;
	pinctrl-names = "default";
	pinctrl-0 = <&pc9202_en_pin>;
        wd-en-gpio = <&gpio4 RK_PD1 GPIO_ACTIVE_HIGH>;
    };

	pca9555: gpio@21 {
               status = "disabled";
               compatible = "nxp,pca9555";
               reg = <0x21>;
               gpio-controller;
               #gpio-cells = <2>;
               gpio-group-num = <200>;
       };


	usbc0: fusb302@22 {
		compatible = "fcs,fusb302";
		reg = <0x22>;
		interrupt-parent = <&gpio0>;
		interrupts = <RK_PC4 IRQ_TYPE_LEVEL_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&usbc0_int>;
		vbus-supply = <&vbus5v0_typec_pwr_en>;
		status = "disabled";

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				usbc0_role_sw: endpoint@0 {
					remote-endpoint = <&dwc3_0_role_switch>;
				};
			};
		};

		usb_con: connector {
			compatible = "usb-c-connector";
			label = "USB-C";
			data-role = "dual";
			power-role = "dual";
			try-power-role = "sink";
			op-sink-microwatt = <1000000>;
			sink-pdos =
				<PDO_FIXED(5000, 1000, PDO_FIXED_USB_COMM)>;
			source-pdos =
				<PDO_FIXED(5000, 3000, PDO_FIXED_USB_COMM)>;

			altmodes {
				#address-cells = <1>;
				#size-cells = <0>;

				altmode@0 {
					reg = <0>;
					svid = <0xff01>;
					vdo = <0xffffffff>;
				};
			};

			ports {
				#address-cells = <1>;
				#size-cells = <0>;

				port@0 {
					reg = <0>;
					usbc0_orien_sw: endpoint {
						remote-endpoint = <&usbdp_phy0_orientation_switch>;
					};
				};

				port@1 {
					reg = <1>;
					dp_altmode_mux: endpoint {
						remote-endpoint = <&usbdp_phy0_dp_altmode_mux>;
					};
				};
			};
		};
	};

};


&pinctrl {
	leds {
		led_user: led-user {
			rockchip,pins = <4 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		led_power: led-power {
			rockchip,pins = <4 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

       headphone {
               hp_det: hp-det {
                       rockchip,pins = <1 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
               };
       };

       hym8563 {
               hym8563_int: hym8563-int {
                       rockchip,pins = <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_up>;
               };
       };

	usb-typec {
		usbc0_int: usbc0-int {
			rockchip,pins = <0 RK_PC4 RK_FUNC_GPIO &pcfg_pull_up>;
		};

	};

    wdt-pwr {
        wdt_pwr_drv: wdt-pwr-drv {
            rockchip,pins = <4 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };

	wdt-en {
        pc9202_en_pin: pc9202-en-pin {
            rockchip,pins = <4 RK_PD1 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };

   ds18b20-dp {
        ds18b20_dp_pin: ds18b20-dp-pin {
            rockchip,pins = <4 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };
    core-mb-sys-en {
        core_mb_sys_en_drv: core-mb-sys-en-drv {
            rockchip,pins = <3 RK_PC1 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };

    sdmmc {
        sdmmc_pwr: sdmmc-pwr {
            rockchip,pins = <3 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
        };
    };

};
